{
  "name": "robust-frontend",
  "version": "1.0.0",
  "author": "Victor Zhuravlev <vzhuravlev15@gmail.com>",
  "license": "GPL version 3",
  "scripts": {
    "build": "rm -rf build && NODE_ENV=production webpack --mode production --env production",
    "dev": "NODE_ENV=development webpack serve --mode development --env development",
    "lint": "npm run lint:code && npm run lint:types",
    "lint:code": "npm run lint:code:styles && npm run lint:code:source",
    "lint:code:styles": "stylelint './src/**/*.{ts,tsx}'",
    "lint:code:source": "eslint './src/**/*.{ts,tsx}'",
    "lint:types": "npm run lint:types:check && npm run lint:types:coverage",
    "lint:types:check": "tsc --noEmit",
    "lint:types:coverage": "type-coverage --ignore-catch --at-least 99 --project src",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@babel/runtime": "7.12.5",
    "core-js": "3.8.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "regenerator-runtime": "0.13.7"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "6.1.1",
    "css-loader": "5.0.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "fork-ts-checker-webpack-plugin": "6.0.4",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "linaria": "2.0.2",
    "lint-staged": "10.5.2",
    "mini-css-extract-plugin": "1.3.1",
    "prettier": "2.2.1",
    "react-hot-loader": "4.13.0",
    "stylelint": "13.8.0",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "type-coverage": "2.14.5",
    "typescript": "4.1.2",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
